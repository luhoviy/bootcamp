{
  "name": "bootcamp-proj",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:fe": "ng serve --proxy-config proxy.config.json",
    "start:be": "npm run dev --prefix api",
    "start": "concurrently \"npm run start:fe\" \"npm run start:be\"",
    "build": "ng build",
    "build:start-be": "npm run build && npm run start:be",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI\n",
    "prepare": "husky install",
    "refactor": "pretty-quick --staged",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "app:install": "npm install && cd api && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "^13.0.2",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "~13.0.0",
    "@angular/material": "^13.0.2",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@ngrx/effects": "^13.0.1",
    "@ngrx/store": "^13.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ngx-spinner": "^13.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@ngrx/schematics": "^13.0.1",
    "@ngrx/store-devtools": "^13.0.1",
    "@types/faker": "^5.5.9",
    "@types/jasmine": "~3.10.0",
    "@types/lodash": "^4.14.177",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-ngrx": "^2.0.0",
    "faker": "^5.5.3",
    "husky": "^7.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ngrx-store-logger": "^0.2.4",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "typescript": "~4.4.3"
  }
}
